{
  "name": "@se-2/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS='--localstorage-file=./.node-localstorage/store' next build",
    "check-types": "tsc --noEmit --incremental",
    "codegen": "PONDER_GRAPHQL_URL=${PONDER_URL:-http://localhost:42069}/graphql graphql-codegen --config codegen.yml",
    "codegen:watch": "PONDER_GRAPHQL_URL=${PONDER_URL:-http://localhost:42069}/graphql graphql-codegen --config codegen.yml --watch",
    "dev": "NODE_OPTIONS='--localstorage-file=./.node-localstorage/store' next dev",
    "format": "prettier --write . '!(node_modules|.next|contracts)/**/*'",
    "ipfs": "NEXT_PUBLIC_IPFS_BUILD=true yarn build && yarn bgipfs upload config init -u https://upload.bgipfs.com && CID=$(yarn bgipfs upload out | grep -o 'CID: [^ ]*' | cut -d' ' -f2) && [ ! -z \"$CID\" ] && echo 'üöÄ Upload complete! Your site is now available at: https://community.bgipfs.com/ipfs/'$CID || echo '‚ùå Upload failed'",
    "lint": "next lint",
    "serve": "NODE_OPTIONS='--localstorage-file=./.node-localstorage/store' next start",
    "start": "NODE_OPTIONS='--localstorage-file=./.node-localstorage/store' next dev",
    "vercel": "vercel --build-env YARN_ENABLE_IMMUTABLE_INSTALLS=false --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env VERCEL_TELEMETRY_DISABLED=1",
    "vercel:login": "vercel login",
    "vercel:yolo": "vercel --build-env YARN_ENABLE_IMMUTABLE_INSTALLS=false --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env NEXT_PUBLIC_IGNORE_BUILD_ERROR=true --build-env VERCEL_TELEMETRY_DISABLED=1",
    "test:e2e": "node test/test-ip-attachment-e2e.mjs"
  },
  "dependencies": {
    "@heroicons/react": "~2.1.5",
    "@lit-protocol/auth": "^8.2.3",
    "@lit-protocol/constants": "^9.0.1",
    "@lit-protocol/lit-client": "^8.3.1",
    "@lit-protocol/lit-node-client-nodejs": "8.0.0-alpha.0",
    "@lit-protocol/networks": "^8.4.1",
    "@rainbow-me/rainbowkit": "2.2.9",
    "@react-native-async-storage/async-storage": "~2.2.0",
    "@scaffold-ui/components": "^0.1.8",
    "@scaffold-ui/debug-contracts": "^0.1.7",
    "@scaffold-ui/hooks": "^0.1.6",
    "@simplewebauthn/browser": "^13.2.2",
    "@tanstack/react-query": "~5.59.15",
    "@types/formidable": "^3.4.6",
    "@walletconnect/modal": "^2.7.0",
    "blo": "~1.2.0",
    "burner-connector": "0.0.20",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "daisyui": "5.0.9",
    "ethers": "^6.16.0",
    "formidable": "^3.5.4",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "graphql-tag": "^2.12.6",
    "js-base64": "^3.7.8",
    "kubo-rpc-client": "~5.0.2",
    "next": "~15.2.8",
    "next-api-decorators": "^2.0.2",
    "next-nprogress-bar": "~2.3.13",
    "next-themes": "~0.3.0",
    "qrcode.react": "~4.0.1",
    "react": "~19.2.3",
    "react-dom": "~19.2.3",
    "react-hot-toast": "~2.4.0",
    "reflect-metadata": "^0.2.2",
    "siwe": "^3.0.0",
    "swagger-ui-react": "^5.31.2",
    "usehooks-ts": "~3.1.0",
    "viem": "2.39.0",
    "wagmi": "2.19.5",
    "zustand": "~5.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.7",
    "@graphql-codegen/typescript-graphql-request": "^6.0.1",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@tailwindcss/postcss": "4.0.15",
    "@trivago/prettier-plugin-sort-imports": "~4.3.0",
    "@types/node": "~18.19.50",
    "@types/react": "~19.0.7",
    "@types/swagger-ui-react": "^5",
    "abitype": "1.0.6",
    "autoprefixer": "~10.4.20",
    "bgipfs": "~0.0.12",
    "eslint": "~9.23.0",
    "eslint-config-next": "~15.2.3",
    "eslint-config-prettier": "~10.1.1",
    "eslint-plugin-prettier": "~5.2.4",
    "postcss": "~8.4.45",
    "prettier": "~3.5.3",
    "tailwindcss": "4.1.3",
    "type-fest": "~4.26.1",
    "typescript": "~5.8.2",
    "vercel": "~39.1.3"
  },
  "packageManager": "yarn@3.2.3"
}
